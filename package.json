{
  "name": "orquestra-utils",
  "version": "0.5.10",
  "main": "dist/orquestra-utils.esm.js",
  "module": "dist/orquestra-utils.esm.js",
  "description": "JavaScript utility library to interact with Orquestra BPM forms",
  "scripts": {
    "dev": "rollup -c -w",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint src/**/*.js --max-warnings=0 --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.js": "npm run lint"
